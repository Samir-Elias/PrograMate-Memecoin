{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_NAME": "PrograMate",
    "NEXT_PUBLIC_APP_URL": "https://programate-memecoin.vercel.app"
  },
  "functions": {
    "src/pages/api/**/*.ts": {
      "maxDuration": 10
    }
  }
}
